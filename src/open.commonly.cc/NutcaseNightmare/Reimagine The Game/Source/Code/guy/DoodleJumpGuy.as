package guy{	import flash.display.MovieClip;	public class DoodleJumpGuy extends Guy	{		public var foreverX:int;		public function DoodleJumpGuy(par:MovieClip):void		{			super("Doodlejump",new MovieClip(),par);			jump_speed = 50;		}		override public function handleMove():void		{			if(stuckToGround){				// ON GROUND				if(!slippery){					jumpGrace = 5;					if(canJump){						stuckToGround = false;						vel.add(-acc.ux*jump_speed,-acc.uy*jump_speed);						canJump = false;						jumpGrace = 0;					}				}			}else{				// IN AIR				if(jumpGrace>0){					jumpGrace--;					if(canJump){						vel.add(-acc.ux*jump_speed,-acc.uy*jump_speed);						canJump = false;						jumpGrace = 0;					}				}			}			x = foreverX;		}		override protected function sfxStep():void{}		override protected function sfxLoudStep():void{			_root.sfx.addSound("Doing");		}	}}