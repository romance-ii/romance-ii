package guy{	import flash.display.MovieClip;		public class TeaGuy extends Guy	{		public function TeaGuy(chr:String,mc:MovieClip,par:MovieClip):void{			super(chr,mc,par);			frict_air = 1;		}				var underwater:Boolean;		override public function enterFrame():void		{			super.enterFrame();			if(!gameRoot.endIt && y>gameRoot.maskk.y+40){				vel.y-=5;				vel.x*=0.99;				if(!underwater){					_root.sfx.addSound("Splash",vel.mag/50);				}				underwater = true;			}else{				underwater = false;			}		}	}}