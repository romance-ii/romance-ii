package guy{	import flash.display.MovieClip;	import flash.geom.Point;	public class EarthquakeGuy extends Guy	{		public function EarthquakeGuy(chr:String,mc:MovieClip,par:MovieClip):void		{			super(chr,mc,par);		}		override protected function hitWall(px:Number,py:Number):Boolean{			stagePoint = gameRoot.localToGlobal(new Point(px,py));			return ( gameRoot.wall.hitTestPoint(stagePoint.x,stagePoint.y,true) && !gameRoot.qCont.hitTestPoint(stagePoint.x,stagePoint.y,true) );		}	}}