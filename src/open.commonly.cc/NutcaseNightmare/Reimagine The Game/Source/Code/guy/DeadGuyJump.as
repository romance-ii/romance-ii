package guy {	import math.Vect;	import flash.display.MovieClip;		public class DeadGuyJump extends DeadGuy	{		var acc:Vect = new Vect(0,2);		var gameRoot:MovieClip;		public function DeadGuyJump(g:Guy,bx:Number,by:Number,r:Number,xxx:Number,gR:MovieClip):void		{			super(g);			vel = new Vect(bx,by);			vel.setMag(10);			rotate = r;			gameRoot = gR;			scaleX=xxx;		}				override public function enterFrame():void{			super.enterFrame();			x += vel.x;			y += vel.y;			rotation += rotate;			gameRoot.setGravity( x,y,acc );			vel.add(acc.x,acc.y);			/*if(x<0){				x=0;				vel.x*=-1;			}			if(x>800){				x=800;				vel.x*=-1;			}*/		}			}	}