package guy {	import math.Vect;	import flash.display.MovieClip;	import flash.utils.getDefinitionByName;	import flash.utils.getQualifiedClassName;	import particle.Particle;		public class DeadGuy extends MovieClip implements Particle	{		var vel:Vect;		var rotate:int;		var charr:MovieClip;		var respawnMe:MovieClip;			public function DeadGuy(g:Guy):void		{			respawnMe = g;			vel = g.vel;			rotate = Math.random()*70+30;			if(Math.random()>0.5){				rotate *= -1;			}			var ClassReference:Class = Class(getDefinitionByName(getQualifiedClassName(g.char)));			charr = new ClassReference();			//charr = g.char;			addChild(charr);			charr.gotoAndStop(1);			charr.y+=g.h*0.5;						x = g.x - g.acc.ux*g.h*0.5;			y = g.y - g.acc.uy*g.h*0.5;			rotation = g.rotation;		}				//public function 				var updateYet:Boolean;		public function enterFrame():void{			if(!updateYet && MovieClip(root).cookies.data.unl.youMode){				charr.update();				updateYet = true;			}		}			}	}