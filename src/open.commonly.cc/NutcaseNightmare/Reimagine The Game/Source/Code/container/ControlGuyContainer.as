package container{	import flash.display.MovieClip;	import guy.Guy;	public class ControlGuyContainer extends GuyContainer	{		public var control:int;		public function ControlGuyContainer(mc:MovieClip,index:int=0):void		{			super(mc);			control = index;		}				override public function enterFrame(mc:MovieClip):void		{			for (var gi:int=0; gi < numChildren; gi++) {			 	var g:Guy = Guy(getChildAt(gi));				mc.setGravity(g.x,g.y,g.acc);				with(g){					if(visible){						if( gi==control ){							handleMove();							handleFrame( true );						}else{							handleFrame();						}						handleGround();						handleGuy();					}					enterFrame();				}			}		}	}}