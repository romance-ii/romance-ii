package container{	import flash.display.MovieClip;	import guy.PreloaderGuy;	public class PreloaderGuyContainer extends GuyContainer	{		public function PreloaderGuyContainer(mc:MovieClip):void		{			super(mc);		}		override public function enterFrame(mc:MovieClip):void		{			for (var i:uint=0; i < numChildren; i++) {			 	var g:PreloaderGuy = PreloaderGuy(getChildAt(i));				mc.setGravity(g.x,g.y,g.acc);				with(g){					handleMouse();					handleMove();					handleFrame( true );					handleGround();					handleGuy();				}			}		}	}}