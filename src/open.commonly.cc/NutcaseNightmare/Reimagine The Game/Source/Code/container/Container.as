package container{	import flash.display.MovieClip;	public class Container extends MovieClip	{		public function Container(mc:MovieClip):void		{			mc.addChild(this);		}		public function add(p:MovieClip):void		{			addChild(p);		}		public function kill(p:MovieClip):void		{			removeChild(p);		}		public function clear():void		{			while(numChildren>0){				removeChild(getChildAt(0));			}		}	}}