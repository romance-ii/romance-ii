package container{	import flash.display.MovieClip;	import particle.Particle;	public class ParticleContainer extends Container	{		public function ParticleContainer(mc:MovieClip):void		{			super(mc);		}		public function addParticle(p:MovieClip,xx:int,yy:int,rr:int=0):void		{			p.x = xx;			p.y = yy;			p.rotation = rr;			add(p);		}		public function enterFrame():void		{			for (var i:int=0; i < numChildren; i++) {			 	Particle(getChildAt(i)).enterFrame();			}		}	}}