package container{	import flash.display.MovieClip;		public class FrogInstakillContainer extends InstakillContainer	{		public function FrogInstakillContainer(mClip:MovieClip,gameRoot:MovieClip):void		{			super(mClip,gameRoot);		}		override public function enterFrame():void		{			for( var i:uint=0; i<mc.numChildren; i++ ){				var tmp:MovieClip = MovieClip(mc.getChildAt(i));				if( tmp.currentFrame!=1 ){					tmp.nextFrame();				}			}			super.enterFrame();		}		override protected function hitHandler():void		{			for( var i:uint=0; i<mc.numChildren; i++ ){				var tmp:MovieClip = MovieClip(mc.getChildAt(i));				if( tmp.hitTestObject(gR.gCont.kid( gR.gCont.control )) ){					tmp.gotoAndStop(2);				}			}			super.hitHandler();		}	}}