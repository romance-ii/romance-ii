package ui{	import flash.display.MovieClip;	public class FunctButton extends Button	{		var fOver:Function;		var fClick:Function;		public function FunctButton(mClip:MovieClip,r:MovieClip,f:Function,ff:Function):void		{			fOver = f;			fClick = ff;			super(mClip,r);		}				//var downJustNow:Boolean;		override public function enterFrame():void{			if(mouseIsOver()){				if(MovieClip(callRoot.root).mouse.down){					if(MovieClip(callRoot.root).mouse.click){						//MovieClip(callRoot.root).sfx.addSound("Switch");						MovieClip(callRoot.root).mouse.click = false;						if(!MovieClip(callRoot.root).paused){							MovieClip(callRoot.root).sfx.addSound("Bass",0.5);						}						fClick();						//downJustNow = true;					}				}else{					fOver();					//downJustNow = false;				}			}		}	}}