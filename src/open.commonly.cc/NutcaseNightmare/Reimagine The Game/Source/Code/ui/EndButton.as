package ui{	import flash.display.MovieClip;	public class EndButton extends FunctButton	{		public function EndButton(mClip:MovieClip,r:MovieClip,frm:String):void		{			var f:Function;			if(frm=="Replay"){				 f = function(){ 				 		rSpeed -= 2; 					  };			}else{				 f = function(){ 				 		rSpeed += 2; 					  };			}			var ff:Function = function(){									if(MovieClip(callRoot.root).pause!=null){										if(!MovieClip(callRoot.root).share.visible){											with(MovieClip(callRoot.root).pause){												_parent.paused = false;												if( visible ){													_root.sfx.play();												}												visible = false;											}										}									}									MovieClip(callRoot.root).gotoAndStop(frm);								};			super(mClip,r,f,ff);		}				var rSpeed:Number = 0;		override public function enterFrame():void		{			super.enterFrame();			rSpeed *= 0.95;			mc.rotation += rSpeed;		}	}}