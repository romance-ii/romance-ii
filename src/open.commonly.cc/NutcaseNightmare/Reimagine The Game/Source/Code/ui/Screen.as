package ui{	import flash.display.MovieClip;	public class Screen extends MovieClip	{		var _root:MovieClip;		var soundName:String;		var subt:String;		var scrName:String;		public function screen():void		{			_root = MovieClip(root);			soundName = "Gong";			subt = "";			scrName = _root.currentLevel.toUpperCase();			screenSwitch();			screenInfo( soundName );			screenText();		}		protected function screenSwitch(){}		protected function screenText(){}				protected function screenInfo( soundName:String ):void		{			if(soundName!=null){				//sfx.clear();				try{					_root.sfx.addSound(soundName);				}catch(e:Error){}			}		}			}}