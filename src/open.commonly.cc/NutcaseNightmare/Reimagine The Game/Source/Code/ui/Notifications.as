package ui {		import flash.display.MovieClip;		public class Notifications extends MovieClip {				public var queueArr:Array = new Array();				public function Notifications() { 			x = whereX;			icon.gotoAndStop(1);		}				public function queue(str:String,i:int=1){			queueArr.push( new Array(str,i) );		}				var whereX:int = -300;		var time:int = 0;		public function enterFrame(){			x = (x*3+whereX)*0.25;			if(x+300<1){				x=-300;			}else if(time>0){				time--;			}else{				whereX = -300;			}			if(x==-300 && queueArr.length>0){				var tmp:Array = queueArr.shift();				t.text = tmp[0];				icon.gotoAndStop(tmp[1]);				whereX = 0;				time = 90;			}		}			}	}