package ui {		import flash.display.MovieClip;		public class CharMaker_Select extends MovieClip {				var _root:MovieClip;		var makerRoot:MovieClip;		var index:int;				public function init(i:int,t:String):void		{			makerRoot = MovieClip(parent.parent);			_root = MovieClip(root);			index = i;			type.text=t;						back.init(function(){makerRoot.decSel(index);});			fwd.init(function(){makerRoot.incSel(index);});						// Lock or not.			if(_root.cookies.data.unl.unlArr[MovieClip(parent).currentFrame-1]){				gotoAndStop(1);			}else{				gotoAndStop(2);			}		}				public function enterFrame():void		{			back.enterFrame();			fwd.enterFrame();		}			}	}