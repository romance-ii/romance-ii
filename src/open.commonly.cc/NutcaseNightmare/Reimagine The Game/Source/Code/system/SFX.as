package system{	import flash.media.Sound;	import flash.media.SoundChannel;	import flash.media.SoundTransform;	public class SFX	{		public var sound:Sound;		public var position:uint;		public var channel:SoundChannel;		public var trans:SoundTransform;		public function SFX(s:Sound,p:uint,c:SoundChannel):void		{			sound = s;			position = p;			channel = c;			trans = new SoundTransform();		}	}}