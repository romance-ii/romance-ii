package system{	public class LoopPlayer extends SFXPlayer	{		public function LoopPlayer():void		{			super();			prefix = "Loop_";		}		override public function play():void{			isPlaying = true;			for( var i:uint = 0; i<soundLength; i++ )			{				sounds[i].channel= sounds[i].sound.play(sounds[i].position,int.MAX_VALUE,sounds[i].trans);			}		}		override public function addSound(soundName:String,vol:Number=0.8,pan:Number=0,loops:uint=int.MAX_VALUE):void		{			clear();			try{				super.addSound(soundName,vol,pan,loops);			}catch(e:Error){				//super.addSound("Skipping",vol,pan,loops);			}		}	}}