package particle{	import flash.display.MovieClip;	public class SmokeParticle extends MovieClip implements Particle	{		var rSpeed:int;		var aSpeed:Number;		var initX;		var initY;		public function SmokeParticle():void		{			addChild(new Particle_SmokeMonster());			rSpeed = Math.random()*50-25;			aSpeed = 0.04;			alpha=1;			rotation = Math.round(Math.random()*360);		}		public function enterFrame():void		{			rotation += rSpeed;			alpha-=aSpeed;			if(alpha<0){				MovieClip(parent).kill(this);			}		}	}}