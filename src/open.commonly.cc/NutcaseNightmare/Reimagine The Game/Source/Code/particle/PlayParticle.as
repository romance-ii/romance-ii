package particle{	import flash.utils.getDefinitionByName;	import flash.display.MovieClip;	public class PlayParticle extends MovieClip implements Particle	{		var mc:MovieClip;		public function PlayParticle(p:String,frm:uint=1):void		{			var ClassReference:Class = getDefinitionByName("Particle_"+p) as Class;			mc = new ClassReference();			mc.stop();			addChild(mc);			mc.gotoAndStop(frm);		}		public function enterFrame():void		{			if(mc.currentFrame==mc.totalFrames){				MovieClip(parent).kill(this);			}else{				mc.nextFrame();			}		}	}}