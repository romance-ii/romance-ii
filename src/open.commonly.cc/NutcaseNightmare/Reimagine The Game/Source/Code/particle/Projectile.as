package particle{	import flash.utils.getDefinitionByName;	import flash.display.MovieClip;	import math.Vect;		public class Projectile extends MovieClip implements Particle	{		var mc:MovieClip;		var gameRoot:MovieClip;		var lifeSpan:int = 0;		public function Projectile(p:String,gR:MovieClip):void		{			gameRoot = gR;			var ClassReference:Class = getDefinitionByName("Particle_"+p) as Class;			mc = new ClassReference();			addChild(mc);						vel = new Vect(0,0);			acc = new Vect(0,2);			spin = Math.random()*90-45;		}				public var vel:Vect;		public var acc:Vect;		var spin:int;		public function enterFrame():void		{			if(lifeSpan++>150){				MovieClip(parent).kill(this);			};						x+=vel.x;			y+=vel.y;			vel.add(acc.x,acc.y);			mc.rotation += spin;						gameRoot.setGravity(x,y,acc);		}	}}