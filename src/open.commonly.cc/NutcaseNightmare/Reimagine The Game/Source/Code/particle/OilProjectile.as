package particle{	import flash.display.MovieClip;	import flash.geom.ColorTransform;	public class OilProjectile extends FluidProjectile	{		public function OilProjectile(gR:MovieClip,xx:int,yy:int,vx:int=0,vy:int=0):void		{			super(gR,xx,yy,vx,vy);			var CT:ColorTransform = new ColorTransform();			CT.color = 0x000000;			mc.transform.colorTransform=CT;		}		override protected function splatter():void{			gameRoot.osCont.addOil(x,y,gameRoot);			super.splatter();		}	}}