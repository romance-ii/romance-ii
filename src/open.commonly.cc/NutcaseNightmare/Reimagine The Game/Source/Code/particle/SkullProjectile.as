package particle{	import flash.display.MovieClip;	import flash.geom.Point;	import particle.PlayParticle;		public class SkullProjectile extends Projectile	{		public function SkullProjectile(gR:MovieClip,xx:int,yy:int):void		{			super("Skull",gR);			mc.gotoAndStop( 1 );			x = xx;			y = yy;			vel.set(0,0);		}		var everHit:Boolean = false;		override public function enterFrame():void		{			if( everHit ){				if(mc.currentFrame==mc.totalFrames){					MovieClip(parent).kill(this);				}				mc.nextFrame();			}else{				if( hitWall() ){					everHit = true;					mc.nextFrame();					gameRoot.miscPartCont.addParticle(new PlayParticle("Lightblood"),x,y,mc.rotation);					gameRoot.sCont.addBlood(x,y,gameRoot);				}else{					super.enterFrame();				}			}		}		protected function hitWall():Boolean{			var stagePoint:Point = gameRoot.localToGlobal(new Point(x,y));			return gameRoot.wall.hitTestPoint(stagePoint.x,stagePoint.y,true);		}	}}