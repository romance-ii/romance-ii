package level{	import flash.display.MovieClip;	import container.ControlGuyContainer;	import gamesys.CameraController;	import guy.Guy;	import math.Vect;		public class ControlGuyLevel extends MovieClip implements Level	{		public var _root:MovieClip;		public var gParent:MovieClip;		public var cam:CameraController;		public var gCont:ControlGuyContainer;				public function init():void		{			gParent = MovieClip(parent.parent);			_root = MovieClip(root);			initGuy();		}		public function initGuy():void{}		public function enterFrame():void		{			allFallLolOff();			endGame();			gCont.enterFrame(this);			cam.enterFrame();		}				public function setGravity(xx:int,yy:int,acc:Vect):void{			acc.set(0,2);		}				public function endGame():void{};		public function allFallLolOff():void		{			if(Math.abs(gCont.kid(gCont.control).x)>9000 || Math.abs(gCont.kid(gCont.control).y)>9000 ){				gParent.reset();			}		}	}}