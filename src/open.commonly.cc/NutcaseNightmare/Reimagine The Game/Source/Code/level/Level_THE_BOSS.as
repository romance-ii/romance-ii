package level{	import flash.display.MovieClip;	import guy.Guy;	import gamesys.CameraController;	import container.ControlGuyContainer;	public class Level_THE_BOSS extends MovieClip implements Level	{		var gParent:MovieClip;		public function init():void		{			gParent = MovieClip(parent.parent);			enterFrame();		}				var cutScene:int;		public function enterFrame():void{			if(cutScene==0){				gParent.startScene("TheBossOpening");				cutScene++;			}else{				//if(!MovieClip(root).loop.isPlaying){				MovieClip(root).loop.addSound("Nervous");				//}				gParent.newGame("THE_BOSS_PORTAL");			}		}				public function endGame():void{};			}}