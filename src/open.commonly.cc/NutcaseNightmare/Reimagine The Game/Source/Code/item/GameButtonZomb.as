package item{	import flash.display.MovieClip;	import flash.geom.Point;	public class GameButtonZomb extends GameButton	{		public function GameButtonZomb(f:Function,mClip:MovieClip,gR:MovieClip,r:Boolean=true,t:uint=10)		{			super(f,mClip,gR,r,t);		}				override protected function hitGuy():Boolean{			if( super.hitGuy() ){				return true;			}else{				stagePoint = gameRoot.localToGlobal(new Point(x,y));				return gameRoot.zombCont.hitTestPoint(stagePoint.x,stagePoint.y,true);			}		}	}}