package item{	import flash.display.MovieClip;		public class BPPelicanFly extends BPPelican	{		var flySpeed:int;		public function BPPelicanFly(mClip:MovieClip,gR:MovieClip):void		{			super(mClip,gR);			flySpeed = 10;			rotation -= 90;		}		override public function enterFrame():void		{			x += Math.sin(rotation*(Math.PI/180))*flySpeed;			y -= Math.cos(rotation*(Math.PI/180))*flySpeed;			rotation+=5;			super.enterFrame();		}	}}