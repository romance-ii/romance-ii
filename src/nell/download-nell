#!/bin/bash
curl http://rtw.ml.cmu.edu/resources/results/08m/ \
  | grep href \
  | perl -ne 'print "http://rtw.ml.cmu.edu/resources/results/08m/$1\n" if m[href="(NELL.*(sv|sv.gz))"]' \
  | sort \
  | uniq \
  | while read uri
    do
      wget -c $uri
    done

echo "Done downloading. Decompressing someâ€¦"
for file in *.gz
do
  gzip -dc $file > $(basename $file .gz) &
done
